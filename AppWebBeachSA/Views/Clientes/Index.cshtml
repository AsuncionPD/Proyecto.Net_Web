    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <style>
        .hero {
            background: url(https://www.vivaplaya.net/viva/wp-content/uploads/2022/04/amanecer-playa-del-carmen-viva-playa.jpg) no-repeat center center/cover;
            height: 60vh;
            display: flex;
            align-items: center;
            justify-content: center;
            color: black;
            text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.7);
        }

            .hero h1, .hero p, .hero a {
                transition: transform 0.3s ease, opacity 0.3s ease;
            }

            .hero:hover h1 {
                transform: scale(1.05);
            }

        .room-item:hover img {
            transform: scale(1.1);
            transition: transform 0.5s ease;
        }

        * {
            box-sizing: border-box;
        }

        body {
            margin: 0;
            padding: 0;
            overflow-x: hidden;
        }

        img, .swiper-container {
            max-width: 100%;
            height: auto;
        }

        .service:hover {
            transform: scale(1.05);
            transition: transform 0.3s ease;
            box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);
        }

        .gallery img:hover {
            transform: scale(1.1);
            transition: transform 0.3s ease;
        }

        .room-item {
            height: 500px;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            border: none;
            outline: none;
            border-radius: 0;
            overflow: hidden;
            padding: 15px;
            background-color: transparent;
            box-shadow: none;
        }

            .room-item img {
                width: 100%;
                height: 100%;
                object-fit: cover;
                border-radius: 0px;
            }

        .product-description {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            height: auto;
            padding: 10px;
            background: rgba(0, 0, 0, 0.8);
            color: #fff;
            border-radius: 10px;
        }


        .card {
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }

            .card:hover {
                transform: translateY(-10px);
                box-shadow: 0 8px 20px rgba(0, 0, 0, 0.2);
            }

    </style>
</head>
