    <header class="hero">
        <div class="text-center text-black mb-lg-5">
            <h1 class="display-5" data-aos="zoom-in">Welcome to Hotel Beach</h1>
            <p class="lead" data-aos="fade-up" data-aos-delay="200">Discover luxury, comfort, and unforgettable experiences.</p>
            <a href="#services" class="btn btn-primary btn-lg" data-aos="zoom-in" data-aos-delay="400">Explore More</a>
        </div>
    </header>

    <section id="room" class="padding-medium">
        <div class="container-fluid padding-side" data-aos="fade-up">

            <h4 class="display-3 fw-normal text-center mt-3">Explore our packages</h4>
            <div class="swiper room-swiper mt-5">
                <div class="swiper-wrapper">
                    @for (int i = 0; i < ViewBag.Paquetes.Count; i++)
                    {
                        var item = ViewBag.Paquetes[i];
                        var image = i + "Room.jpg";

                        <div class="swiper-slide">
                            <div class="room-item position-relative rounded-5 overflow-hidden">
                                <img src="~/imgs/@image" alt="img" class="post-image img-fluid rounded-4">
                                <div class="product-description position-absolute p-5 text-start justify-content-center">
                                    <h4 class="display-6 fw-normal text-white">@item.Nombre</h4>

                                    <table>
                                        <tbody>
                                            <tr class="text-white">
                                                <td class="pe-2">Price:</td>
                                                <td class="price">@item.PrecioPorPersonaPorNoche /Per persont</td>
                                            </tr>
                                            <tr class="text-white">
                                                <td class="pe-2">Percentage:</td>
                                                <td>@item.PrimaPorcentaje</td>
                                            </tr>
                                            <tr class="text-white">
                                                <td class="pe-2">Monthly payments</td>
                                                <td>@item.Mensualidades</td>
                                            </tr>
                                            <tr class="text-white">
                                                <td class="pe-2">Services:</td>
                                                <td>Wifi, Television, Bathroom,...</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <a asp-action="paquete" asp-controller="Reservaciones" asp-route-id="@item.PaqueteID">
                                        <p class="text-decoration-underline text-white m-0 mt-2">Browse Now</p>
                                    </a>
                                </div>
                            </div>
                            <div class="room-content text-center mt-3">
                                @Html.ActionLink($"{@item.Nombre}", "paquete", new { id = item.PaqueteID, controller = "Reservaciones" }, new { @class = " display-6 fw-normal text-darkgoldenrod text-decoration-none" })
                                <p class="text-darkgoldenrod"><span class="text-black fs-4">@item.PrecioPorPersonaPorNoche</span>/person</p>
                            </div>
                        </div>

                    }

                </div>
                <div class="swiper-pagination room-pagination position-relative mt-5"></div>
            </div>
        </div>
    </section>


    <section id="reservations" class="py-5">
        <div class="container">
            <h4 class="display-3 fw-normal text-center mt-3" data-aos="fade-up">My Reservations</h4>
            <div class="row g-4 mt-3">
                @foreach (var item in Model) {
                    var fechaSalida = item.FechaReservacion.AddDays(item.Noches);
                    var image = (@item.PaqueteID - 1) + "Room.jpg";
                    var idModal = "R" + item.ReservacionID;

                    <div class="col-12 col-md-6 col-lg-4" data-aos="fade-up" data-aos-delay="100">
                        <div class="card border-0 shadow-sm rounded-4">
                            <img src="~/imgs/@image" class="card-img-top rounded-top-4" alt="Room Image">
                            <div class="card-body">
                                <h5 class="card-title fw-bold">Deluxe Room</h5>
                                <p class="card-text text-muted mb-1">
                                    <i class="bi bi-calendar-check me-2"></i><strong>Check-in:</strong> @item.FechaReservacion
                                </p>
                                <p class="card-text text-muted mb-3">
                                    <i class="bi bi-calendar-x me-2"></i><strong>Check-out:</strong> @fechaSalida
                                </p>
                                <p class="card-text">
                                    <i class="bi bi-person-fill me-2"></i><strong>Guests:</strong> @item.Personas person
                                </p>
                                <div class="d-flex justify-content-between mt-4">
                                    <button class="btn btn-primary bg-gray mt-3 btn-lg d-flex align-items-center justify-content-center w-100 me-2" data-bs-toggle="modal" data-bs-target="#@idModal">
                                        <i class="bi bi-eye me-2"></i>View
                                    </button>
                                    <button class="btn btn-primary bg-gray mt-3 btn-lg d-flex align-items-center justify-content-center w-100 me-2">
                                        <i class="bi bi-pencil me-2"></i>Modify
                                    </button>
                                    <button class="btn btn-primary bg-gray mt-3 btn-lg d-flex align-items-center justify-content-center w-100 me-2">
                                        <i class="bi bi-trash me-2"></i>Cancel
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                 
                    <!-- Modal -->
                    <div class="modal fade" id=@idModal tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLabel">Información del libro </h5>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                </div>
                                <div class="modal-body">
                                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                    <div class="row">
                                        <div class="form-group col-md-6">
                                            <label class="control-label">Package</label>
                                            <input readonly value="@item.PaqueteID" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label">Reservation Date</label>
                                            <input readonly value="@item.ReservacionID" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label">Nights</label>
                                            <input readonly value="@item.Noches" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label ">Number of People</label>
                                            <input readonly value="@item.Personas" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label ">Total Without Discount</label>
                                            <input readonly value="@item.TotalSinDescuento" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label  class="control-label ">Discount Percentage</label>
                                            <input readonly value="@item.DescuentoPorcentaje" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label">Total With Discount</label>
                                            <input readonly value="@item.TotalConDescuento" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label  class="control-label ">Payment Method</label>
                                            <input readonly value="@item.FormaPago" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label ">Check Number</label>
                                            <input readonly value="@item.NumeroCheque" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label  class="control-label ">Bank</label>
                                            <input readonly value="@item.Banco" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label ">Down Payment</label>
                                            <input readonly value="@item.Prima" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label  class="control-label ">Remaining in Monthly Payments</label>
                                            <input readonly value="@item.RestoEnMensualidades" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label ">Exchange Rate</label>
                                            <input readonly value="@item.TipoCambio" class="form-control mt-2" />
                                        </div>

                                        <div class="form-group col-md-6">
                                            <label class="control-label ">Total in USD</label>
                                            <input readonly value="@item.TotalUSD" class="form-control mt-2" />
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-primary bg-gray mt-3 btn-lg " data-bs-dismiss="modal">Close</button>
                                </div>
                            </div>
                        </div>
                    </div>

                }
                

 
            </div>

            <div class="text-center mt-5" data-aos="fade-up" style="display: none;" id="no-reservations">
                <p class="text-muted">You have no reservations at the moment.</p>
                <a href="#services" class="btn btn-outline-primary">Explore Our Rooms</a>
            </div>

            <div class="text-center mt-4" data-aos="fade-up">
                <button class="btn btn-dark btn-lg px-5">
                    <i class="bi bi-plus-circle me-2"></i>Create a Reservation
                </button>
            </div>
        </div>
    </section>


